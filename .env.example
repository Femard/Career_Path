# Application Settings
APP_NAME=Dynamic Career Pathing SaaS
APP_VERSION=0.1.0
DEBUG=False

# Neo4j Database
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-password-here
NEO4J_DATABASE=neo4j

# External API Keys
THEIRSTACK_API_KEY=your-theirstack-api-key
CORESIGNAL_API_KEY=your-coresignal-api-key
NUMBEO_API_KEY=your-numbeo-api-key
LEVELSFYI_API_KEY=your-levelsfyi-api-key
LINKEDIN_LEARNING_API_KEY=your-linkedin-learning-api-key
COURSERA_API_KEY=your-coursera-api-key

# Security
SECRET_KEY=generate-a-secure-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:8000

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Cache TTL (seconds)
CACHE_TTL_MARKET_DATA=3600
CACHE_TTL_SALARY_DATA=86400
CACHE_TTL_LMS_DATA=43200
